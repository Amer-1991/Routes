<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>My Funding Requests</title>
</head>
<body>
    <h1>My Funding Requests</h1>
    <ul>
        <% requests.forEach(function(request) { %>
            <li><%= request.purpose %> - <%= request.amount %> - Status: <%= request.status %></li>
        <% }) %>
    </ul>
</body>
</html>