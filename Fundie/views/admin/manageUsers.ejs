<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <title>Manage Users</title>
</head>
<body>
    <h1>Manage Users</h1>
    <ul>
        <% users.forEach(function(user) { %>
            <li><%= user.username %> - Role: <%= user.role %></li>
        <% }) %>
    </ul>
</body>
</html>